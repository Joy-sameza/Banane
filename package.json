{
  "name": "banane",
  "version": "1.2.1",
  "type": "module",
  "description": "An app to help manage Bananes",
  "scripts": {
    "start": "node dist/app.js",
    "watch:ts": "tsc --watch",
    "watch:app": "node --watch-path=./src dist/app.js",
    "dev": "concurrently --kill-others \"npm run watch:ts\" \"npm run watch:app\"",
    "build": "tsc"
  },
  "keywords": [
    "Banane"
  ],
