  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.17",
    "@types/sqlite3": "^3.1.11",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie": "^0.6.0",
    "dotenv": "^16.4.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-windows": "^1.0.0-beta.8",
    "sqlite3": "^5.1.7"
  }
